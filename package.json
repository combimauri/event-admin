{
  "name": "weekend-coding",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "keys:create": "cat ./src/environments/keys.template.ts > ./src/environments/keys.ts",
    "keys:create-prod": "cat ./src/environments/keys.template.ts > ./src/environments/keys.prod.ts",
    "keys:inject": "echo $KEYS > ./src/environments/keys.ts",
    "keys:inject-prod": "echo $PROD_KEYS > ./src/environments/keys.prod.ts",
    "firebase:deploy": "firebase deploy -P default --only hosting:admin",
    "firebase:deploy-prod": "firebase deploy -P prod --only hosting:admin",
    "firebase:build-deploy": "npm run build && npm run firebase:deploy",
    "firebase:build-deploy-prod": "ng run build:prod && npm run firebase:deploy-prod",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.8",
    "@angular/common": "^14.2.8",
    "@angular/compiler": "^14.2.8",
    "@angular/core": "^14.2.8",
    "@angular/fire": "^7.4.1",
    "@angular/forms": "^14.2.8",
    "@angular/platform-browser": "^14.2.8",
    "@angular/platform-browser-dynamic": "^14.2.8",
    "@angular/router": "^14.2.8",
    "@angular/service-worker": "^14.2.8",
    "@zxing/browser": "^0.1.1",
    "@zxing/library": "^0.19.1",
    "@zxing/ngx-scanner": "^3.6.2",
    "angular-star-rating": "^6.0.0",
    "angularx-qrcode": "^14.0.0",
    "css-star-rating": "^1.3.1",
    "firebase": "^9.13.0",
    "jspdf": "^2.5.1",
    "materialize-css": "^1.0.0",
    "rxjs": "~6.6.7",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.7",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "^14.2.7",
    "@angular/compiler-cli": "^14.2.8",
    "@angular/language-service": "^14.2.8",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jspdf": "^2.0.0",
    "@types/materialize-css": "^1.0.11",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "eslint": "^8.23.1",
    "firebase-tools": "^11.15.0",
    "husky": "^8.0.1",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "protractor": "~7.0.0",
    "ts-custom-error": "^3.2.0",
    "ts-node": "~7.0.0",
    "tslib": "^2.4.0",
    "typescript": "~4.6.4"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{js,ts,scss,html}": "prettier --write"
  }
}
